{
  "rewrites": [
    {
      "source": "/wallet-service/:path*",
      "destination": "https://mapi.examtree.ai/wallet-service/:path*"
    },
    {
      "source": "/auth-service/:path*",
      "destination": "https://mapi.examtree.ai/auth-service/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
